<h1>Edit Organisation</h1>

<div class= "form">
<%= form_for @organisation do |o| %>
    <%= o.label :name %><br>
    <%= o.text_field :name %><br>
    <%= o.label :hourly_rate %><br>
    <%= o.text_field :hourly_rate %><br>
    <%= o.submit "Edit"%><br>
    <% end %>
    

    </div>
    <%= link_to  "Organisations" , organisations_path %>