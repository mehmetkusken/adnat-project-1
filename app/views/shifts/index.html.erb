<h1>Shift List</h1>
<table>
<tr>
    <th>Name</th>
    <th>Start</th>
    <th>Finish</th>
    <th>Created</th>
    <th>Break</th>
  </tr>
<% @shifts.each do |s| %>

  <tr>
    <td> <%= s.user.name %></td>
    <td> <%= s.start %></td>
    <td> <%= s.finish %></td>
    <td> <%= s.created_at %></td>
    <td> <%= s.break_length %></td>
  </tr>
  

     <% end %>
     </table>  
<%= link_to "Create a Shift" , new_shift_path %>